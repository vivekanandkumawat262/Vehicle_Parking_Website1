var lazyImg = masonryGrid ? masonryGrid.querySelector('[data-perf="lazy-loaded-img"]') : null;

eagerImgs.forEach(listenToImgOnLoad(imgOnLoadListenerEager));
if (lazyImg !== null) {
  listenToImgOnLoad(imgOnLoadListenerLazy)(lazyImg);
}
})();
